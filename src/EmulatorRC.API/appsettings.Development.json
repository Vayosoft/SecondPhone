{
  "Kestrel": { //https://thecodeblogger.com/2021/05/07/certificates-and-limits-for-asp-net-core-kestrel-web-server/
    //"EndpointDefaults": {
    //  "Protocols": "Http1AndHttp2"
    //},
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5003",
        "Protocols": "Http1AndHttp2"
      },
      "gRPC": {
        "Url": "http://localhost:5004",
        "Protocols": "Http2"
      },
      //"Https": {
      //  "Url": "https://localhost:5004",
      //  "Certificate": {
      //    "Path": "<path to .pfx file>",
      //    "Password": "<certificate password>"
      //  }
      //},
      //"HttpsDefaultCert": {
      //  "Url": "http://localhost:5004",
      //  "Protocols": "Http2"
      //}
    },
    //"Certificates": {
    //  "Default": {
    //    "Path": "<path to .pfx file>",
    //    "Password": "<certificate password>"
    //  }
    //},
    "Limits": {
      "MaxConcurrentConnections": 100,
      "MaxConcurrentUpgradedConnections": 100
    },
    "DisableStringReuse": true
  },
  "Jwt": { //https://blog.devgenius.io/jwt-authentication-in-asp-net-core-e67dca9ae3e8
    "Symmetric": {
      "Key": ""
    },
    "Asymmetric": {
      "PrivateKey": "",
      "PublicKey": ""
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Exceptions", "Serilog.Sinks.File", "Serilog.Enrichers.Thread" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      },
      {
        "Name": "Debug",
        "Args": {
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "WithExceptionDetails" ],
    "Properties": {
      "Environment": "localDev"
    }
  }
}

